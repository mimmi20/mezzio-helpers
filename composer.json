{
  "name": "mimmi20/mezzio-navigation-helpers",
  "type": "library",
  "description": "Provides helpers for Mezzio Navigation",
  "homepage": "https://github.com/mimmi20/mezzio-navigation-helpers",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.0 || ^8.0.0",
    "ext-mbstring": "*",
    "laminas/laminas-json": "^3.2.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.2.0",
    "mimmi20/mezzio-generic-authorization": "^2.0.1",
    "mimmi20/mezzio-navigation": "^2.0.3"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0",
    "mezzio/mezzio-authorization": "*"
  },
  "require-dev": {
    "ext-intl": "*",
    "infection/infection": "^0.21.5",
    "laminas/laminas-config": "^3.5.0",
    "laminas/laminas-i18n": "^2.11.0",
    "laminas/laminas-servicemanager": "^3.6.4",
    "mimmi20/coding-standard": "^2.1.0",
    "pepakriz/phpstan-exception-rules": "^0.11.6",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.82",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.18",
    "phpunit/phpunit": "^9.5.4"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-config": "^3.5.0, to provide page configuration (optional, as arrays and Traversables are also allowed)",
    "laminas/laminas-i18n": "^2.11.0, required to use the Translator",
    "laminas/laminas-servicemanager": "^3.6.4, to use the navigation factories"
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\Navigation\\Helper\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\Navigation\\Helper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\Navigation\\Helper\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-navigation-helpers/issues",
    "source": "https://github.com/mimmi20/mezzio-navigation-helpers"
  }
}
